{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "mdc/tabs",
	"version": "0.1.0",
	"title": "Structure en tabulation",
	"category": "dreets",
	"icon": "ellipsis",
	"description": "Ce bloc permet d'avoir une structure de tabulation pour afficher plusieurs onglets de contenu sur la page.",
	"supports": {
		"html": false
	},
	"textdomain": "mdc-tabs",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"styles": [
        { "name": "default", "label": "Default", "isDefault": true },
        { "name": "other", "label": "Other" }
    ],
	"attributes": {
		"content": {
			"type": "string",
			"source": "html",
			"selector": "div"
		},
		"tabs": {
			"type": "array",
			"selector": "div"
		},
		"style":{
			"type": "string",
			"default": "none"	
		}
	}
}
